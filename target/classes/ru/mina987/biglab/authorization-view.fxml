<?xml version="1.0" encoding="UTF-8"?>

<?import javafx.scene.control.Button?>
<?import javafx.scene.control.ComboBox?>
<?import javafx.scene.control.Label?>
<?import javafx.scene.layout.HBox?>
<?import javafx.scene.layout.VBox?>

<VBox alignment="CENTER" minHeight="121.0" minWidth="350.0" prefHeight="121.0" prefWidth="350.0" spacing="10.0" xmlns="http://javafx.com/javafx/21" xmlns:fx="http://javafx.com/fxml/1" fx:controller="ru.mina987.biglab.controllers.MainController">
   <children>
      <Label text="%choice" />
      <ComboBox fx:id="cbAuth" prefWidth="220.0" />
      <HBox alignment="CENTER" spacing="30.0">
         <children>
            <Button mnemonicParsing="false" onAction="#onConfirm" text="%confirm" />
            <Button mnemonicParsing="false" onAction="#onExit" text="%exit" />
         </children>
      </HBox>
   </children>
</VBox>
